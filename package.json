{
  "name": "sample",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "types": "lib",
  "scripts": {
    "lint": "eslint . --ext .ts --ext .json",
    "launch": "cross-env TS_NODE_PROJECT='./tsconfig.json' ts-node ./launcher.ts",
    "prettier": "prettier --write --loglevel warn \"**/**/*.ts\"",
    "test": "rimraf reports1/* && npm run report:history && npm run prettier && npm run launch",
    "dev": "npm run prettier && npm run launch",
    "report:history": "mkdir -p reports/allure-reporter/allure-report/history reports/allure-reporter/allure-results/history && cp -a reports/allure-reporter/allure-report/history reports/allure-reporter/allure-results/",
    "clean-report": "rimraf reports/allure-reporter/allure-results && rimraf logs && rimraf reports/json-reporter",
    "run-report": "allure generate reports/allure-reporter/allure-results/ -o reports/allure-reporter/allure-report/ --clean && allure open reports/allure-reporter/allure-report/ -p 8000",
    "report": "npm run clean-report && npm run test ; npm run run-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-plugin-json": "^3.1.0",
    "ts-node": "^8.10.1",
    "typescript": "4.5.4",
    "chromedriver": "^111.0.0",
    "cross-env": "^7.0.2",
    "fs-extra": "^10.1.0",
    "reflect-metadata": "^0.1.13",
    "yargs": "^16.0.3",
    "@types/mocha": "^7.0.2",
    "@types/node-fetch": "^2.6.1",
    "@types/chai": "^4.2.3",
    "@wdio/allure-reporter": "7.14.1",
    "@wdio/cli": "7.16.10",
    "@wdio/dot-reporter": "7.16.6",
    "@wdio/local-runner": "7.16.10",
    "@wdio/mocha-framework": "7.16.6",
    "@wdio/spec-reporter": "7.16.9",
    "@wdio/sync": "7.16.10",
    "allure-commandline": "^2.13.0",
    "tsconfig-paths": "^3.9.0",
    "wdio-chromedriver-service": "7.2.0",
    "wdio-json-reporter": "^2.0.0",
    "webdriverio": "7.16.10",
    "fibers": "5.0.1",
    "axe-core": "^3.3.2",
    "chai": "^4.2.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.7.1"
  }
